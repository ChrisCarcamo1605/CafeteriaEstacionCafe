    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./infrastructure/db/Connection.ts",
